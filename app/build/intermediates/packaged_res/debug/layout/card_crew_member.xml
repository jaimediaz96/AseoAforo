<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="110dp"
    android:layout_marginBottom="25dp"
    android:background="@drawable/shape_border"
    android:padding="25dp"
    tools:context=".ui.activities.CrewActivity"
    tools:layout_width="500dp">

    <TextView
        android:id="@+id/tvCewMemberName"
        style="@style/Theme.AseoAforos.tvLabelStyle.size18"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Jaime Eduardo Díaz tobón" />

    <ImageButton
        android:id="@+id/imgBtnCrewMemberDelete"
        style="@style/Theme.AseoAforos.btnDelete"
        android:layout_width="60dp"
        android:layout_height="match_parent"
        android:contentDescription="@string/delete"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>